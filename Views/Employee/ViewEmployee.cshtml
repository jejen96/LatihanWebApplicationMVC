@model List<LatihanWebApplicationMVC.Models.Employee.Employee>
@{
}

<h1>Employee</h1>
<table class="table">   
    <thead>
        <tr>
            <th>Id</th>
            <th>Nama</th>
            <th>Alamat</th>
            <th>Usia</th>
            <th>Tanggal Lahir</th>
            <th>Edit</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
         <tr>
                <td>@employee.Id</td>
                <td>@employee.Nama</td>
                <td>@employee.Alamat</td>
                <td>@employee.Usia</td>
                <td>@employee.TanggalLahir</td>
                <td><a href="EditView/@employee.Id">Edit</a></td>
                

         </tr>   
        }
    </tbody>

</table>
