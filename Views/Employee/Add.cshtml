@model LatihanWebApplicationMVC.Models.AddEmployeeViewModel

@{
}
<h1>Add Employee</h1>
<form method="post" action="Add" class="mt-5">
    <div class="form-group">
        <label for="nama">nama</label>
        <input type="text" class="form-control" placeholder="nama" asp-for="Nama">
    </div>
    <div class="form-group">
        <label for="alamat">alamat</label>
        <input type="text" class="form-control" placeholder="alamat" asp-for="Alamat">
    </div>
    <div class="form-group">
        <label for="usia">usia</label>
        <input type="text" class="form-control" placeholder="usia" asp-for="Usia">
    </div>
    <div class="form-group">
        <label for="tanggallahir">tanggallahir</label>
        <input type="date" class="form-control" placeholder="tanggallahir" asp-for="TanggalLahir">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>